{
  "manifest_version": 2,

  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0.0",
  "default_locale": "en",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [{
    "matches": [
    	"https://app.asana.com/*"
    ],
   "js":["main.js"],
   "run_at": "document_idle"
  }],
  "permissions": [
    	"https://app.asana.com/"
  ]
}
